    "jotai": "^2.0.4",
    "nanoid": "^4.0.2",
    "socket.io-client": "^4.6.1"
  },
  "devDependencies": {
    "@types/react": "17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  }
