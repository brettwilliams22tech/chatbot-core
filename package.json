{
  "name": "@pulsehq/chatbot-core",
  "version": "1.0.0",
  "description": "Socket client to connect to rasa",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": {
        "default": "./src/index.ts"
